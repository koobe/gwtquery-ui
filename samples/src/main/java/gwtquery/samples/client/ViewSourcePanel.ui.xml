<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <g:DisclosurePanel width="95%">
    <g:header>View Source</g:header>
    <g:FlowPanel>
      <g:HTMLPanel>
        <fieldset class="source" title="HTML Source">
          <legend>HTML Source</legend>
          <pre>
            <code>
              <g:Label ui:field="htmlSource">Html</g:Label>
            </code>
          </pre>
        </fieldset>
        <fieldset class="source" title="Java Source">
          <legend>Java Source</legend>
          <pre>
            <code>
              <g:Label ui:field="javaSource">Java</g:Label>
            </code>
          </pre>
        </fieldset>
      </g:HTMLPanel>
    </g:FlowPanel>
  </g:DisclosurePanel>

</ui:UiBinder>